{
  "name": "stureflect-backend-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:watch": "jest --watch",
    "test:integration": "node test-integration.js",
    "dev": "nodemon server.js",
    "build": "echo \"Build complete - Express app is ready\"",
    "start": "node server.js"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/public/**",
      "!src/docs/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brend-VanDenEynde/stureflect-backend-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Brend-VanDenEynde/stureflect-backend-api/issues"
  },
  "homepage": "https://github.com/Brend-VanDenEynde/stureflect-backend-api#readme",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
